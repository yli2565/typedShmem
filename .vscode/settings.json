  "testMate.cpp.test.advancedExecutables": [
    {
      "pattern": "{build,Build,BUILD,out,Out,OUT}/**",
      "cwd": "${workspaceFolder}",
      "runTask": {
        "before": [
          "Make - Test"
        ]
      },
    }
  ],
  "testMate.cpp.test.parallelExecutionLimit": 4,
  "testMate.cpp.debug.breakOnFailure": true,
  "testMate.cpp.debug.configTemplate": {
    "type": "cppvsdbg",
    "linux": {
      "type": "cppdbg",
      "MIMode": "gdb"
    },
    "darwin": {
      "type": "cppdbg",
      "MIMode": "lldb"
    },
    "program": "${exec}",
    "args": "${argsArray}",
    "cwd": "${cwd}",
    "env": "${envObj}",
    "environment": "${envObjArray}",
    "sourceFileMap": "${sourceFileMapObj}",
    "preLaunchTask": "Make - Test" // Task to execute before launching the debugger
  }
